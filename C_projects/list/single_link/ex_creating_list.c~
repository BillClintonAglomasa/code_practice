#include <stdio.h>
#include <stdlib.h>

/**
 * main - Entry point 
 *
 * Return: Always 0 (Success)
 *
 * Description: Create a linked list
 */

struct node
{
  int a;
  struct node *link;
};

int main(void)
{
  struct node *head = NULL;
  head = malloc(sizeof(struct node));
  head->a = 45;
  head->link = NULL;

  printf("%d", head->a);
  
  return(0);
}
